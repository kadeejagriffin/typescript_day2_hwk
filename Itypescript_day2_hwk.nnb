            ]
        },
        {
            "language": "typescript",
            "source": [
                "function getPrice(shirtSize:Size): number {\n    if (shirtSize == 0){\n        return 9.99\n    } else if (shirtSize == 1){\n        return 10.99\n    }else{\n        return 11.99\n    }\n}"
            ],
            "outputs": []
        },
